{
    "name": "nicolasbeauvais/botscout-client",
    "description": "A PHP client library for botscout.com",
    "keywords": [
        "botscout",
        "client",
        "bot",
        "protection"
    ],
    "homepage": "https://github.com/nicolasbeauvais/botscout-client",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Beauvais",
            "email": "nicolasbeauvais1@gmail.com",
            "homepage": "http://nicolas-beauvais.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "NicolasBeauvais\\BotScout\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NicolasBeauvais\\BotScout\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
